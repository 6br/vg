# baseURI: http://biohackathon.org/resource/pantograph

@prefix : <http://biohackathon.org/resource/pantograph#> .
@prefix vg: <http://biohackathon.org/resource/vg#> .
@prefix odgi: <http://biohackathon.org/resource/odgi#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix spin: <http://spinrdf.org/spin#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix edam: <http://edamontology.org/> .

<http://biohackathon.org/resource/pantograph>
  rdf:type owl:Ontology ;
  <http://purl.org/dc/terms/license> <http://creativecommons.org/publicdomain/zero/1.0/> ;
  spin:imports <http://topbraid.org/spin/owlrl-all> ;
  spin:imports <http://topbraid.org/spin/rdfsplus> ;
  spin:imports <http://topbraid.org/spin/spinowl> ;
  owl:versionInfo "Created at the CoVID-19 Hackathon"^^xsd:string ;
.

:Component
  rdf:type owl:Class ;
  rdfs:comment "A region between Arrows shared by all individuals."^^xsd:string ;
  rdfs:label "Component"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
.
:Arrow
  rdf:type owl:Class ;
  rdfs:comment "An arrow marks a graph traversal along a nonlinear connection."^^xsd:string ;
  rdfs:label "Arrow"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
.
:Step
  rdf:type owl:Class ;
  rdfs:comment "A step along a path in the pantograph. A step points to a :Component with supplementl informations."^^xsd:string ;
  rdfs:label "Step"^^xsd:string ;
  rdfs:subClassOf owl:Thing ;
.
:arrival
  rdf:type owl:ObjectProperty ;
  rdfs:comment "A bin in , representing a sequence section."^^xsd:string ;
  rdfs:domain :Arrow ;
  rdfs:label "goes to"^^xsd:string ;
  rdfs:range odgi:Bin ;
.
:departure
  rdf:type owl:ObjectProperty ;
  rdfs:comment "A bin in , representing a sequence section."^^xsd:string ;
  rdfs:domain odgi:Bin ;
  rdfs:label "goes to"^^xsd:string ;
  rdfs:range :Arrow ;
.
:participates
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This means that the path participates the arrow between components."^^xsd:string ;
  rdfs:domain vg:Path ;
  rdfs:label "participates"^^xsd:string ;
  rdfs:range :Arrow ;
.
:occupies
  rdf:type owl:ObjectProperty ;
  rdfs:comment "This means that the path occupies the component."^^xsd:string ;
  rdfs:domain vg:Path ;
  rdfs:label "occupies"^^xsd:string ;
  rdfs:range :Step ;
.
:coverage
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The coverage in the component of the path."^^xsd:string ;
  rdfs:domain :Step ;
  rdfs:label "coverage"^^xsd:string ;
  rdfs:range xsd:float ;
.
:inversionRate
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The inversion rate in the component of the path."^^xsd:string ;
  rdfs:domain :Step ;
  rdfs:label "inversionRate"^^xsd:string ;
  rdfs:range xsd:float ;
.
:firstNucleotide
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the position on the path at which this step starts."^^xsd:string ;
  rdfs:domain :Step ;
  rdfs:label "firstPosition"^^xsd:string ;
  rdfs:range xsd:float ;
.
:lastNucleotide
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "This is the position on the path at which this step ends."^^xsd:string ;
  rdfs:domain :Step ;
  rdfs:label "lastPosition"^^xsd:string ;
  rdfs:range xsd:float ;
.
:firstBin
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The first bin of the component."^^xsd:string ;
  rdfs:domain :Component ;
  rdfs:label "firstBin"^^xsd:string ;
  rdfs:range odgi:Bin ;
.
:lastBin
  rdf:type owl:DatatypeProperty ;
  rdfs:comment "The last bin of the component."^^xsd:string ;
  rdfs:domain :Component ;
  rdfs:label "lastBin"^^xsd:string ;
  rdfs:range odgi:Bin ;
.
